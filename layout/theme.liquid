  <script type="text/javascript" defer="">
    
    // CLOSE DIV WHEN CLICKED OUTSIDE OF IT
    $(document).mouseup(function (e) {
      var container = $('.cart-box'); // mini cart
      var infoPopup = $('.hidden-info'); // information popup
      
      // if the clicked target is not the mini cart or any of its descendents
      if (!container.is(e.target) && container.has(e.target).length == 0 ) {
        container.animate({
          marginRight: "-100%",
        });
        
        $('body').removeClass('active-panel');
      }

      // if the clicked target is not the information popup or any of its descendents
      if (!infoPopup.is(e.target) && infoPopup.has(e.target).length == 0 ) {
        infoPopup.removeClass('show');
      }
    });
    
    // BACK TO TOP FOR MOBILE DEVICES ONLY
    if ($(window).width() < 980) {
      $(document).scroll(function() {
        var y = $(this).scrollTop();
        if (y > 500) {
          $('#backToTop').fadeIn();
        } else {
          $('#backToTop').fadeOut();
        }
      });
    }
    
    $('#backToTop').click(function() {
      $('html, body').animate({
        scrollTop: 0,
      }, 1000);
    });

    // INFORMATION POPUP CLOSE
    $('.close-icon').on('touchend', function() {
      $(this).parent().removeClass('show');
    });
    
    // show header bar on session init
    (function() {
      var headerBarSession = sessionStorage.getItem('hideHeaderBar');
      if (!headerBarSession) {
        $('.announcement-bar').slideDown(250);
      }
    })();

    // Header bar hide till session is cleared
    $('.announcement-bar__close').click(function() {
      $('.announcement-bar').slideUp(250);
      sessionStorage.setItem('hideHeaderBar', true);
    });

  </script>
